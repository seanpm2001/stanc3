  (ppx_deriving (= 4.2.1))
  (fmt (= 0.8.5))
  (yojson (= 1.7.0))
  (ocamlformat (and :with-test (= 0.8)))
  (merlin (and :with-test (= 3.4.1)))
  (utop :with-test)
  (ocp-indent :with-test)
  (patdiff :with-test)
 )
